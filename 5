positioning=input()
letter1=positioning[0]
number1=int(positioning[1])
letter2=positioning[3]
number2=int(positioning[4])
letter_1=ord(letter1)-ord('a')+1
letter_2=ord(letter2)-ord('a')+1
if (abs(letter_1-letter_2)==1 and 
    abs(number1-number2)==2) or (abs(letter_1-letter_2)==2 and 
                                                     number1-number2==1):
  print('верно')
else:
  print('ошибка')
  
